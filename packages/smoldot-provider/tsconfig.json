{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "outDir": "dist",
     "paths": {
       "*": ["./types/*"]
    },
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "esnext"
  },
  "include": ["src"],
  "exclude": ["src/examples/ws.ts"]
}
