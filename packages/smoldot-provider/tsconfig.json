{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": ".",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "composite": true,
    "module": "ES2020",
    "target": "ES2018"
  },
  "include": ["src","."],
  "files": [
    "package.json"
  ]
}
